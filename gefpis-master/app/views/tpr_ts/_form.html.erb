<%= simple_form_for(@tpr_t) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :week_no %>
    <%= f.association :tutee_t %>
    <%= f.input :strengths %>
    <%= f.input :weaknesses %>
    <%= f.input :actions %>
    <%= f.association :module_t %>
    <%= f.association :member_t %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
