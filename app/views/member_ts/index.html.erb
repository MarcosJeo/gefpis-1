<p id="notice"><%= notice %></p>

<h1>Listing Member Ts</h1>
<%= link_to 'New Member t', new_member_t_registration_path %>
<table>
  <thead>
    <tr>
      <th>Id number</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Middle initial</th>
      <th>Email address</th>
      <th>Contact no</th>
      <th>Attendance</th>
      <th>Type</th>
      <th>Committee</th>
      <th>Birthday</th>
      <th>Year course</th>
      <th>Scholarship</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_ts.each do |member_t| %>
      <tr>
        <td><%= member_t.id_number %></td>
        <td><%= member_t.first_name %></td>
        <td><%= member_t.last_name %></td>
        <td><%= member_t.middle_initial %></td>
        <td><%= member_t.email %></td>
        <td><%= member_t.contact_no %></td>
        <td><%= member_t.attendance %></td>
        <td><%= member_t.membership %></td>
        <td><%= member_t.committee %></td>
        <td><%= member_t.birthday %></td>
        <td><%= member_t.year_course %></td>
        <td><%= member_t.scholarship %></td>
        <%if current_member_t == member_t or current_member_t.membership == "Administration" %>
        <td><%= link_to 'Edit', edit_member_t_registration_path(member_t.id) %></td>
        <td><%= link_to "Delete", destroy_member_t_path(member_t), confirm: "Are you sure?", :method => :delete %><td>
        <%end%>
 
      </tr>
    <% end %>
  </tbody>
</table>

<br>


