
  <div class="col s12">
      <center>
        <br>
        <br>
        <a class="main-green-text crete-round"><font size="6">Tutee Profile</font></a>          
        <br>
        <img src="images/line2.png" height="55">
        <br>
      </center>
    </div>



<%= simple_form_for(@tutee_t) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :school %>
    <%= f.input :math_grade %>
    <%= f.input :eng_grade %>
    <%= f.input :father_name %>
    <%= f.input :father_occupation %>
    <%= f.input :mother_name %>
    <%= f.input :mother_occupation %>
    <%= f.input :ambition %>
    <%= f.input :fav_subject %>
    <%= f.input :fav_food %>
    <%= f.input :fav_color %>
    <%= f.input :fav_movie %>
    <%= f.input :attendance %>
    <%= f.association :module_t, label: 'Current Module', as: :select %>
    <%= f.input :group_no %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

</html>